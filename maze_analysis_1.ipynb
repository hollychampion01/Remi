def find_closest_node_rrt(graph, target_x, target_y):
    min_dist = float('inf')
    closest_id = None
    for node_id, node in graph.nodes.items():
        dist = math.sqrt((node.y - target_x)**2 + (node.x - target_y)**2)
        if dist < min_dist:
            min_dist = dist
            closest_id = node_id
    return closest_id

